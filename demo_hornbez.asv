% DEMO : how to use 'hornbez'% initialisationt = [0:0.01:1];           % parameter value(s)cp_x = [0 0.5 3.5 4.0];   % x-values of control pointscp_y = [0 1.5 -1.0 1.0];   % y-values of control points% computationx = hornbez(3, cp_x, t);  % compute x-values of Bézier curvey = hornbez(3, cp_y, t);  % compute y-values of Bézier curve% plotting plot(cp_x,cp_y,'ko--',... % control points     x, y, 'b');          % Bézier curve  axis([-2 5 -2 5]);